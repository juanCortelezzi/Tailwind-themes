{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Next.js",
  "extends": "./base.json",
  "compilerOptions": {
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "./next-env.d.ts",
    "./**/*.ts",
    "./**/*.tsx",
    "./**/*.cjs",
    "./**/*.mjs",
    "./.next/types/**/*.ts"
  ],
  "exclude": ["node_modules"]
}
